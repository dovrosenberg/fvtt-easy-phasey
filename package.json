{
  "name": "fvtt-easy-phasey",
  "version": "0.1.0",
  "private": true,
  "description": "Foundry VTT v13 module to manage phased maps.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dovrosenberg/fvtt-easy-phasey.git"
  },
  "author": "Dov Rosenberg",
  "homepage": "https://github.com/dovrosenberg/fvtt-easy-phasey#readme",
  "main": "main.ts",
  "scripts": {
    "build": "vite build",
    "debug": "vite build --minify=false --mode development",
    "lint": "eslint --ext .ts,.vue src",
    "linkdata": "ln -s $PWD/dist ~/foundry13data/Data/modules/easy-phasey",
    "tsc": "vue-tsc --skipLibCheck -p ./tsconfig.json",
    "unlinkdata": "unlink ~/foundry13data/Data/modules/easy-phasey",
    "foundry": "node ~/foundry13/resources/app/main.js --dataPath=/home/dov/foundry13data"
  },
  "devDependencies": {
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@types/fvtt-types": "github:League-of-Foundry-Developers/foundry-vtt-types#main",
    "@types/jquery": "^3.5.32",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "mini-css-extract-plugin": "^2.9.2",
    "node-sass": "^9.0.0",
    "null-loader": "^4.0.1",
    "prettier": "^3.4.2",
    "raw-loader": "^4.0.2",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-scss": "^4.0.1",
    "sass": "^1.83.0",
    "sass-loader": "^16.0.4",
    "semver-increment": "^1.0.1",
    "source-map-loader": "^5.0.0",
    "string-replace-loader": "^3.1.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.3",
    "vite": "^6.1.6",
    "vite-plugin-env-compatible": "^2.0.1",
    "vite-plugin-html": "3.2.2"
  }
}
